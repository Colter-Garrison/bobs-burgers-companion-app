{
	"expo": {
		"name": "bobs-burgers-companion-app",
		"slug": "bobs-burgers-companion-app",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/bob-favicon.png",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/images/bobs-splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#F8DF24"
		},
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/bobs-splash.png",
				"backgroundColor": "#F8DF24"
			}
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/bob-favicon.png"
		},
		"plugins": ["expo-router"],
		"experiments": {
			"typedRoutes": true
		}
	}
}
